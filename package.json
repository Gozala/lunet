{
  "name": "lunet.link",
  "version": "0.0.1",
  "description": "A local peer agent",
  "scripts": {
    "local": "webrun --allow-require src/local/main.js",
    "remote": "webrun --allow-require src/remote/main.js",
    "certify": "cd .env && openssl req -x509 -out localhost.certificate -keyout localhost.key -newkey rsa:2048 -nodes -sha256  -subj '/CN=localhost' -extensions EXT -config ../localhost.config"
  },
  "dependencies": {
    "@mattiasbuelens/web-streams-polyfill": "0.2.0",
    "@rangermauve/webrun": "3.0.2",
    "mime-types": "2.1.21",
    "tls-keygen": "3.6.1",
    "web-streams-node": "0.3.1",
    "ws": "6.1.2"
  },
  "license": "MPL-2.0"
}
